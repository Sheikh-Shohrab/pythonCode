def printName(name, iD):
    print(f" Your Name Location: {id(name)} Your ID location: {id(iD)}")
    print(f"Hi {name} Your ID: {iD}")
    print("Hi {} Your ID: {}".format(name, iD))
    return

yourName = "Shohrab"
yourID = 2025402003
print(f" Your Name Location: {id(yourName)} Your ID location: {id(yourID)}")
printName(yourName, yourID)